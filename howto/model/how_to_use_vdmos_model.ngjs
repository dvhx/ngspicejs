#!/usr/bin/env ngspicejs
// How to use vdmos model
// linter: ngspicejs-lint
"use strict";

// Define new vdmos model
vdmos_model("MYMOS").kind('VDMOS').vto(2.5).is(1e-14).rg(200); //!
battery('U1', 1, 0, 9);
// use new vdmos model
mosfet_n('T1', 1, 1, 0, 'MYMOS'); //!
echo('Idss', tran().run().avg('I(U1)').toEng());

